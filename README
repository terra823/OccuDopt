from flask import Flask
from flask import request, url_for, render_template
from flaskext.mongoengine import MongoEngine

app = Flask(__name__)
app.config['MONGODB_DB'] = 'occudopt'
db = MongoEngine(app)

from occudopt.models import Relationship, User, Message, Image

u0 = User(email = 'this@that.net')
u1 = User(email = 'yo@mama.net')
m = Message(message_from = u0, message_to = u1, message_body = "this be a test")